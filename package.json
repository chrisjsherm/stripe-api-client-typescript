{
  "name": "stripe-api-client-typescript",
  "version": "0.1.1",
  "description": "Stripe API client written in typescript",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run env && cross-env DEVELOPMENT='true' ts-node-dev --respawn ./src/index.ts",
    "env": "./node_modules/.bin/dotenvx run -- ts-node ./src/environment/environment.main.ts",
    "env:prod:local": "./node_modules/.bin/dotenvx run -f .env.production.local -f .env -- ts-node ./src/environment/environment.main.ts",
    "env:prod:remote": "./node_modules/.bin/dotenvx run -f .env.production.remote -f .env -- ts-node ./src/environment/environment.main.ts",
    "clean": "rimraf dist/*",
    "build": "npm run clean && npm run env:prod:remote && tsc",
    "build:local": "npm run clean && npm run env:prod:local && tsc",
    "server": "node dist/index.js",
    "test": "jest"
  },
  "author": "Sherman Digital LLC",
  "license": "UNLICENSED",
  "dependencies": {
    "@aws-sdk/client-ses": "^3.533.0",
    "@aws-sdk/client-ssm": "^3.533.0",
    "@dotenvx/dotenvx": "^1.6.4",
    "@fusionauth/typescript-client": "^1.48.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.3.0",
    "jose": "^5.2.2",
    "luxon": "^3.4.4",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "sanitize-html": "^2.12.1",
    "stripe": "^14.13.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/luxon": "^3.4.2",
    "@types/sanitize-html": "^2.11.0",
    "aws-sdk-client-mock": "^3.0.1",
    "aws-sdk-client-mock-jest": "^3.0.1",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nock": "^13.5.4",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
