{
  "name": "stripe-api-client-typescript",
  "version": "0.0.1",
  "description": "Stripe API client written in typescript",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run env && cross-env DEVELOPMENT='true' ts-node-dev --respawn ./src/index.ts",
    "env": "ts-node ./src/environment/environment.main.ts",
    "clean": "rimraf dist/*",
    "build": "npm run clean && npm run env && tsc",
    "server": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Sherman Digital LLC",
  "license": "UNLICENSED",
  "dependencies": {
    "@fusionauth/typescript-client": "^1.48.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.2",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.3.0",
    "jose": "^5.2.2",
    "stripe": "^14.13.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
